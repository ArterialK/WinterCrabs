<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/playas"
    tools:context=".ui.gallery.GalleryFragment">

    <TableLayout
        android:id="@+id/tableLayout"
        android:layout_width="348dp"
        android:layout_height="386dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.65999997">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView22"
                android:layout_width="110dp"
                android:layout_height="match_parent"
                tools:src="@drawable/cancun" />

            <ImageView
                android:id="@+id/imageView23"
                android:layout_width="111dp"
                android:layout_height="match_parent"
                tools:src="@drawable/puertoV" />

            <ImageView
                android:id="@+id/imageView24"
                android:layout_width="126dp"
                android:layout_height="94dp"
                tools:src="@drawable/puertoE" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="94dp"
                android:layout_height="94dp"
                tools:src="@drawable/playaO" />

            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="94dp"
                android:layout_height="94dp"
                tools:src="@drawable/playas1" />

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="94dp"
                android:layout_height="94dp"
                tools:src="@drawable/playas2" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView13"
                android:layout_width="94dp"
                android:layout_height="94dp"
                tools:src="@drawable/playas4" />

            <ImageView
                android:id="@+id/imageView14"
                android:layout_width="94dp"
                android:layout_height="94dp"
                tools:src="@drawable/playa5" />

            <ImageView
                android:id="@+id/imageView15"
                android:layout_width="94dp"
                android:layout_height="94dp"
                tools:src="@drawable/playa6" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView16"
                android:layout_width="94dp"
                android:layout_height="94dp"
                tools:src="@drawable/playas7" />

            <ImageView
                android:id="@+id/imageView17"
                android:layout_width="108dp"
                android:layout_height="match_parent"
                tools:src="@drawable/playas8" />

            <ImageView
                android:id="@+id/imageView18"
                android:layout_width="94dp"
                android:layout_height="94dp"
                tools:src="@drawable/playas9" />
        </TableRow>

    </TableLayout>

    <ImageView
        android:id="@+id/imageView25"
        android:layout_width="159dp"
        android:layout_height="130dp"
        android:src="@drawable/logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.074" />

    <TextView
        android:id="@+id/textGallery"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/lugares"
        android:textSize="20sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.022" />

</androidx.constraintlayout.widget.ConstraintLayout>